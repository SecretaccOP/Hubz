Beach Tycoon-
local args = {
	9999,
	"2"
}
game:GetService("ReplicatedStorage"):WaitForChild("E"):WaitForChild("UE"):FireServer(unpack(args))

Bank tycoon-
_G.enabled = true
 
while _G.enabled == true do
    local args = {
	"AddAccount"
}
game:GetService("ReplicatedStorage"):WaitForChild("TaskCompleted"):FireServer(unpack(args))

    wait()
end

Motel simulator-
local args = {
	999999999999999
}
game:GetService("ReplicatedStorage"):WaitForChild("GiveCash"):FireServer(unpack(args))


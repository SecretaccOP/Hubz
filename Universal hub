HubsAndGUISTab:AddButton({
	Name = "Dex Explorer V4",
	Callback = function()
        local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
        local Window = Library.CreateLib("Life in Prison GUI", "Ocean")
        
        local executor = identifyexecutor() or getexecutor() or getexecutorname()
        
        -- Main Tab
        
        local Main = Window:NewTab("Main")
        
        --Player Section(Main)
        
        local CheckEXP1Section = Main:NewSection("Executor Were You Using : "..executor.."")
        
        local MainSection = Main:NewSection("Player")
        
        MainSection:NewButton("Speed (Rejoin To Reset)", "Speed! (also need better version of this plz)", function()
            game:GetService("RunService").Heartbeat:Connect(function()
        if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").MoveDirection.Magnitude > 0 then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = (game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,0,-1.1))
            end
        end)
        end)
        
        MainSection:NewSlider("Gravity", "The Owner of LP is dumb, even this is not bypassed lol", 900, 1, function(v)
            game.Workspace.Gravity = v
        end)
        
        MainSection:NewButton("Inf Jump (C, Z/X To flip)", "A skidded script, Owner Took people's script Lmao", function()
            loadstring(game:HttpGet("https://pastebin.com/raw/7wDcPtLk"))()
        end)
        
        --Wip Section (Main)
        

local AnimationsSection = AnimationsTab:AddSection({
	Name = "R15 Animations"
})

AnimationsTab:AddButton({
	Name = "Dakrai X | R15 Animations",
	Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/XNEOFF/FEAnimationGui/main/FEAnimationGui.lua", true))()
  	end    
})

local MiscTab = Window:MakeTab({
	Name = "Player Tab",
	Icon = "nil",
	PremiumOnly = false
})

local MiscSection = MiscTab:AddSection({
	Name = "Local Player"
})

MiscTab:AddTextbox({
	Name = "Walkspeed",
	Default = "1-200",
	ValueName = "Walkspeed",
	Callback = function(Text)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = (Text)
	end    
})

MiscTab:AddTextbox({
	Name = "JumPower",
	Default = "1-200",
	TextDisappear = false,
	Callback = function(Text)
		game.Players.LocalPlayer.Character.Humanoid.JumPower = (Text)
	end	  
})

MiscTab:AddButton({
	Name = "Infinite Yeild",
	Callback = function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
  	end    
})

local Tab = Window:NewTab("UI Toggles")
local Section = Tab:NewSection("Toggles")

    Section:NewKeybind("Close/Open Ui", "KeybindInfo", Enum.KeyCode.B, function()
        Library:ToggleUI()
    end)

    Section:NewKeybind("TP Hold Z to tp", "KeybindInfo", Enum.KeyCode.Z, function()
        local Plr = game:GetService("Players").LocalPlayer
local Mouse = Plr:GetMouse()
 
Mouse.Button1Down:connect(
    function()
        if not game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.Z) then
            return
        end
        if not Mouse.Target then
            return
        end
        Plr.Character:MoveTo(Mouse.Hit.p)
    end
)
    end)
    

